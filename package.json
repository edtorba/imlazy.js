{
  "name": "imlazy.js",
  "version": "2.0.0",
  "description": "Lightweigght vanilla js plugin to lazyload images.",
  "main": "src/app/app.js",
  "repository": "https://github.com/edtorba/imlazy.js.git",
  "bugs": {
    "url": "https://github.com/edtorba/imlazy.js/issues"
  },
  "author": "Ed Torba",
  "license": "MIT",
  "keywords": [
    "lazyload",
    "lazy",
    "load",
    "responsive",
    "imlazy.js",
    "imlazy",
    "images",
    "image",
    "DOM",
    "preload",
    "on demand",
    "browser",
    "js"
  ],
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "npm run build:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "test": "mocha --compilers js:babel-core/register test/*.js",
    "clean:bin": "rimraf src/bin",
    "clean": "rimraf node_modules src/bin"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jshint": "^2.9.4",
    "jshint-loader": "^0.8.3",
    "mocha": "^3.2.0",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0",
    "webpack-stream": "^3.2.0"
  }
}
