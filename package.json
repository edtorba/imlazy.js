{
  "name": "imlazy.js",
  "version": "2.0.1",
  "description": "A lightweight vanilla JS plugin that lazyloads images and background images.",
  "main": "src/bin/imlazy.pkgd.js",
  "repository": "https://github.com/edtorba/imlazy.js.git",
  "bugs": {
    "url": "https://github.com/edtorba/imlazy.js/issues"
  },
  "author": "Ed Torba",
  "license": "MIT",
  "keywords": [
    "lazyload",
    "lazy",
    "load",
    "responsive",
    "retina",
    "imlazy.js",
    "imlazy",
    "performance",
    "image"
  ],
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "npm run build:dev",
    "start": "webpack --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "clean:bin": "rimraf src/bin",
    "clean": "rimraf node_modules src/bin"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "jshint": "^2.9.4",
    "jshint-loader": "^0.8.3",
    "mocha": "^3.2.0",
    "webpack": "^1.13.2",
    "webpack-merge": "^0.15.0",
    "webpack-stream": "^3.2.0"
  }
}
